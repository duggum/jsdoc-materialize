{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": [ "test" ],
    "includePattern": ".+\\.js(doc)?$",
    "exclude": [ "template", "node_modules", "test/static" ],
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "parser": "marked",
    "tags": ["example", "see"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "staticFiles": {
        "paths": [
          "test/static"
        ]
      }
    },
    "applicationName": "jsdoc-materialize",
    "meta": {
      "title"       : "JSDoc Materialize",
      "description" : "A JSDoc 3 template using the Materialize CSS framework.",
      "keyword"     : "jsdoc, jsdoc3, template, material, materialize"
    },
    "linenums": true
  },
  "opts": {
    "template"  : "dist/jsdoc-materialize",
    "encoding"  : "utf8",
    "private"   : true,
    "recurse"   : true,
    "readme"    : "test/README.md",
    "tutorials" : "test/tutorials",
    "mdPages"   : [
      "./test/pages/contribute.md",
      "./test/pages/credits.md",
      "./test/pages/todo.md",
      "./test/pages/changelog.md",
      "./test/pages/license.md"
    ]
  }
}
